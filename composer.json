{
    "name": "lobotomised/dbbackup",
    "description": "A databases backup tools for laravel",
    "keywords": ["laravel", "database", "backup"],
    "license": "MIT",
    "authors": [
        {
            "name": "lobotomised"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lobotomised\\Dbbackup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lobotomised\\Dbbackup\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lobotomised\\Dbbackup\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
